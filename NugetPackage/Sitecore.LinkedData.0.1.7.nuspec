<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Sitecore.LinkedData</id>
        <version>0.1.7</version>
        <title>Sitecore Linked Data Library</title>
        <authors>adamsimsy</authors>
        <owners>adamsimsy</owners>
        <licenseUrl>https://github.com/Adamsimsy/Sitecore.LinkedData/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/Adamsimsy/Sitecore.LinkedData</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A package that adds Linked Data functionality into Sitecore and allows you (in the future) to select a preferred triple store.

Please note: This package is purely experimental and you should refer to the project page for more information.</description>
        <dependencies>
            <dependency id="dotNetRDF" version="1.0.3" />
            <dependency id="HtmlAgilityPack" version="1.4.6" />
            <dependency id="Newtonsoft.Json" version="5.0.8" />
            <dependency id="VDS.Common" version="1.2.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="lib\LinkedData.dll" target="lib" />
		<file src="content\SitecoreLinkedData.config" target="content\App_Config\Include\SitecoreLinkedData.config" />
		<file src="content\LinkedDataInstaller.cs" target="content\App_Start\LinkedDataInstaller.cs" />
		<file src="content\Gallery Links.xml" target="content\sitecore\shell\Applications\Content Manager\Galleries\Links\Gallery Links.xml" />
    </files>
</package>